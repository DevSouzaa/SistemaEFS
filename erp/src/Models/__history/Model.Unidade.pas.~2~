unit Model.Unidade;

interface

uses
  System.SysUtils;

type
  TUnidade = class
  public
    IDUnidade: Integer;
    Descricao: string;
    IDUsuarioCadastro: Integer;
    IDUsuarioAlteracao: Integer;
    Fracao: Integer;
    Sigla: string;
    Ativo: Boolean;
    DataHoraAlteracao: TDateTime;
    DataHoraCadastro: TDateTime;
  end;

implementation

end.

